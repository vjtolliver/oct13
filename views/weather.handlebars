<section class= "" id="savedlctns" >

    <button class="autolctn">Washington DC</button>
    <button class="autolctn">Oxon Hill</button>
    <button class="autolctn">Woodbridge</button>
        
      </section>

      <!-- Section .container-weather row col-?-9 -->
    <div class="container-weather row col-md-12">
        <!-- Selected City Api Code -->
        <section id="current-city">
            <h1 class="city-name"></h1>
            <p id="date"></p>
            <p class="date"></p>
            <br>
            <div id="weathericon"> 
                <img id="weather-icon" src=""> 
            </div>
            <br>
            <p class="city-temp"></p>
            <p class="humidity"></p>
            <p class="wind-speed"></p>
        </section>
        <br>
        <!-- Heading 3 - 5-Day Forecast: -->
        <!-- 5-Day API Code -->
        <section id="fiveday">
            <h3 col-md-2>Five Day Forecast: </h3>
            <br>
            <div class="row">
            
            <section class="day-zero col-md-1"></section>
            <section class="day-one col-md-2">
            <p class="d-one-icon">
                <img id="d-one-icon" src="">
            </p>
            <br>
            <p class="d-one-temp"></p>
            <p class="d-one-hum"></p>
            <p class="d-one-wind"></p>
            </section>

            <section class="day-two col-md-2">
                <p class="d-two-icon">
                    <img id="d-two-icon" src="">
                </p>
            <br>
            <p class="d-two-temp"></p>
            <p class="d-two-hum"></p>
            <p class="d-two-wind"></p>
            </section>

            <section class="day-thr col-md-2">
                <p class="d-thr-icon">
                    <img id="d-thr-icon" src="">
                </p>
            <br>
            <p class="d-thr-temp"></p>
            <p class="d-thr-hum"></p>
            <p class="d-thr-wind"></p>
            </section>

            <section class="day-four col-md-2">
                <p class="d-four-icon">
                    <img id="d-four-icon" src="">
                </p>
            <br>
            <p class="d-four-temp"></p>
            <p class="d-four-hum"></p>
            <p class="d-four-wind"></p>
            </section>

            <section class="day-five col-md-2">
                <p class="d-five-icon">
                    <img id="d-five-icon" src="">
                </p>
            <br>
            <p class="d-five-temp"></p>
            <p class="d-five-hum"></p>
            <p class="d-five-wind"></p>
            </section>

            <section class="day-na col-md-1"></section>
            </div>
        </section>
        
    </div>
    
